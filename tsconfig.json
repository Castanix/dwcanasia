{
    "compilerOptions": {
        "target": "ES6",
        "module": "ESNext",
        "allowJs": true,
        "jsx": "react-jsx",
        "strict": true,
        "moduleResolution": "Node",
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
    },

    "include": [
        "src",
        "src/logo.svg"
    ]
}